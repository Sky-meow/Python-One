from ..views.api import index
from django.urls import path,re_path,include

app_name = 'api'
urlpatterns = [
    path('index',index),
]

from django.conf import settings
if 'material' in settings.INSTALLED_APPS:
    urlpatterns.append(re_path(r'material', include('material.urls.api')))